{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "es6",
    "lib": ["es6", "es2017"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./net/ts",
    "composite": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
